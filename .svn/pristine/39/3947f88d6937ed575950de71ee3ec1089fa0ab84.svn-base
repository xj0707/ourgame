<div class="page-content">
    <div class="col-sm-6 pull-right">
    
        <!--<span class="btn btn-sm btn-primary pull-right" onclick="javascript:window.location.href = 'addtemplate'">-->
           <!--新增模板-->
            <!--<i class="icon-reply icon-only"></i>-->
        <!--</span>-->
       
    </div>
    <div class="page-header">
        <h1>
            推锅模板时间设置表
        </h1>
    </div><!-- /.page-header -->

    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
            <div class="row">
                <div class="col-xs-12">
                    <table id="simple-table" class="table  table-bordered table-hover">
                        <thead>
                            <tr>
                                <th class="hidden-480">序号</th>
                                <th >下注时间</th>
                                <th >搓牌时间</th>
                                <th class="hidden-480">所属</th>
                                <th>操作</th>
                            </tr>
                        </thead>

                        <tbody>
                            {volist name="infos" id="vo" key="k"}
                            <tr>
                                <td>{$k}</td>
                                <td>{$vo.un32BetWaitSecond}</td>
                                <td>{$vo.un32RubCardSecond}</td>
                                <td class="hidden-480">线路号：{$vo.merchantId}</td>
                                <td>
                                    <div class="hidden-sm hidden-xs btn-group">
                                         <a href='{:url("edittime","id=$vo[id]")}'>
                                           <span class="btn btn-xs btn-info">
                                                <i class="ace-icon fa fa-pencil bigger-120">修改</i>
                                            </span>
                                        </a>
                                    </div>

                                    <div class="hidden-md hidden-lg">
                                        <div class="inline pos-rel">
                                            <span class="btn btn-minier btn-primary dropdown-toggle" data-toggle="dropdown" data-position="auto">
                                                <i class="ace-icon fa fa-cog icon-only bigger-110"></i>
                                            </span>

                                            <ul class="dropdown-menu dropdown-only-icon dropdown-yellow dropdown-menu-right dropdown-caret dropdown-close">

                                                 <li>
                                                    <a href='{:url("edittime","id=$vo[id]")}' class="tooltip-success" data-rel="tooltip" title="修改">
                                                        <span class="blue">
                                                            <i class="ace-icon fa fa-pencil-square-o bigger-120"></i>
                                                        </span>
                                                    </a>
                                                </li>

                                            </ul>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            {/volist}    
                        </tbody>
                    </table>
                     {$infos->render()}
                </div>
            </div>           
        </div>
    </div>
</div>



