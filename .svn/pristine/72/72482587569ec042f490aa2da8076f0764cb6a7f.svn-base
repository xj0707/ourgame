// 提交表单
function check_form()
{
    var jinbi = $.trim($('#jinbi').val());
    var zuansi     = $.trim($('#zuansi').val());
    var userid=$("#user_id").val();
    if(!userid)
    {
        alert('操作异常！请重新操作');
        return false;
    }
    if(!jinbi && !zuansi){
        alert('你还没有选择金币或房卡');
        return false;
    }
    if(zuansi){
        if(isNaN(zuansi)){
            alert("房卡数据不合法");
            return false;
        }
        if(zuansi<=0){
            alert('房卡数量不能小于0');
            return false;
        }
    }
    if(zuansi>=10000000000){
        alert('房卡数量过多');
        return false;
    }
    if(jinbi){
        if(isNaN(jinbi)){
            alert("金币的数据不合法");
            return false;
        }
        if(jinbi<=0){
            alert('金币数量不能小于0');
            return false;
        }
    }

    if(jinbi>=10000000000){
        alert('金币数量太大');
        return false;
    }
    var form_data = $('#form_data').serialize();
    // 异步提交数据到action/add_action.php页面
    $.ajax(
            {
                url: "<?php echo url('addcoins'); ?>?userid="+userid,
                data:{"form_data":form_data},
                type: "post",
//                beforeSend:function()
//                {
//                    $("#tip").html("<span style='color:blue'>正在处理...</span>");
//                    return true;
//                },
                success:function(data)
                {
                    if(data > 0)
                    {
                        //$("#tip").html("<span style='color:blueviolet'>恭喜操作成功！</span>");
                        // document.location.href='system_notice.php'
                        alert("购买成功");
                        window.location.href='index';
                    }
                    else
                    {
                        // $("#tip").html("<span style='color:red'>失败，请重试</span>");
                        alert('购买失败');
                        location.reload();
                    }
                },
                error:function()
                {
                    alert('请求出错');
                    location.reload();
                },
                complete:function()
                {
                    $('#acting_tips').hide();
                }
            });

    return false;
}
